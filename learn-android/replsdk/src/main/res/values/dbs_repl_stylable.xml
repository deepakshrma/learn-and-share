<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DBSChatToolbarView">
        <attr name="android:drawableStart"/>
        <attr name="android:drawableEnd"/>
    </declare-styleable>
    <declare-styleable name="DBSLoadingDots">
        <attr name="dbs_auto_play" format="boolean"/>

        <attr name="dbs_dots_color" format="color"/>
        <attr name="dbs_dots_count" format="integer"/>
        <attr name="dbs_dots_size" format="dimension"/>
        <attr name="dbs_dots_space" format="dimension"/>

        <attr name="dbs_loop_duration" format="integer"/>
        <attr name="dbs_loop_start_delay" format="integer"/>

        <attr name="dbs_jump_duration" format="integer"/>
        <attr name="dbs_jump_height" format="dimension"/>
    </declare-styleable>
</resources>